import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        int i;
        int c=0;
        for(i=0;i<n;i++){
            a[i]=s.nextInt();
            int s_a=(int)Math.sqrt(a[i]);
            if(s_a*s_a==a[i])
                c++;
        }
        System.out.println(c);            
  }
}
