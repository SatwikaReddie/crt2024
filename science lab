import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        int i;
        for(i=0;i<n;i++){
            a[i]=s.nextInt();
        }
        Arrays.sort(a);
        int s1=0;
        for(i=n-1;i>=n-2;i--){
            s1=s1+a[i];
        }     
        System.out.println(s1);
    }
}
