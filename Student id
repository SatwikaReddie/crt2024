import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        int i;
        for(i=0;i<n;i++){
            a[i]=s.nextInt();
        }
        Arrays.sort(a);
        for(i=0;i<n;i+=2){
            System.out.print(a[i]+" ");
        }
        for(i=1;i<n;i+=2){
            System.out.print(a[i]+" ");
        }
    }
}
